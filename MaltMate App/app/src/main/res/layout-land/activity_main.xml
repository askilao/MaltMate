<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    tools:context="com.example.aao.maltmate.MainActivity"
    android:background="#330C00"
    android:orientation="horizontal"
    android:paddingRight="5dp">


    <ImageButton
        android:id="@+id/findwhiskeyButton"
        android:background="@drawable/testmenufind"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:layout_marginBottom="10dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_weight="0.5"
        android:onClick="onClick"/>
    <ImageButton
        android:id="@+id/myWhiskeyButton"
        android:background="@drawable/testmenumy"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:layout_marginBottom="10dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_weight="0.5"
        android:onClick="onClick"/>




</LinearLayout>
